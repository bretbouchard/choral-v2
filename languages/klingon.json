  "special_pronunciations": {
    "tlh": {
      "description": "Voiceless alveolar lateral affricate",
      "ipa": "/tɬ/",
      "features": "lateral release, distinctive Klingon sound",
      "synthesis_note": "Requires lateral formant configuration"
    },
    "Q": {
      "description": "Uvular affricate",
      "ipa": "/q͡χ/",
      "features": "back of tongue to uvula, then fricative release",
      "synthesis_note": "Lower F2, wider bandwidths for guttural quality"
    },
    "q": {
      "description": "Voiceless uvular stop",
      "ipa": "/q/",
      "features": "back of tongue to uvula",
      "synthesis_note": "Lower formants, stronger burst"
    },
    "H": {
      "description": "Voiceless uvular fricative",
      "ipa": "/χ/",
      "features": "back of tongue to uvula, continuous",
      "synthesis_note": "Lower formants, continuous noise"
    },
    "gh": {
      "description": "Voiced velar fricative",
      "ipa": "/ɣ/",
      "features": "voiced, back of tongue",
      "synthesis_note": "Similar to H but with voicing"
    },
    "S": {
      "description": "Voiceless retroflex fricative",
      "ipa": "/ʂ/",
      "features": "tongue curled back",
      "synthesis_note": "Higher F3 than standard /ʃ/"
    },
    "D": {
      "description": "Voiced retroflex stop",
      "ipa": "/ɖ/",
      "features": "tongue curled back",
      "synthesis_note": "Retroflex formant lowering"
    },
    "I": {
      "description": "Near-close front unrounded vowel",
      "ipa": "/ɪ/",
      "features": "slightly lower than /i/",
      "note": "Always short, never stressed"
    }
  },

  "cultural_notes": {
    "tone": "Aggressive and guttural",
    "tempo": "Faster than most languages",
    "articulation": "Precise and forceful",
    "synthesis_recommendation": "Use Subharmonic mode for 'opera' style Klingon choirs"
  }
