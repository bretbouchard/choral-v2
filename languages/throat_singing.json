  "technique_description": {
    "overview": "Throat singing is a vocal technique where a single vocalist produces multiple pitches simultaneously through precise control of vocal tract resonances and subharmonic vibration of ventricular folds.",
    "mechanism": "The vocalist maintains a low drone pitch (fundamental) while shaping the vocal tract to emphasize specific formant frequencies that emerge as distinct melodic tones. Some styles also engage the ventricular (false) vocal folds to create subharmonics at integer ratios (2:1, 3:1, 4:1).",
    "primary_styles": [
      {
        "name": "Sygyt",
        "origin": "Tuva",
        "characteristics": "High-pitched whistle-like melody, bright formant around 1800-2000 Hz",
        "synthesis": "Narrow F2 formant, high amplitude, sharp resonance"
      },
      {
        "name": "Kargyraa",
        "origin": "Tuva",
        "characteristics": "Deep sub-bass with 3:1 subharmonic, ventricular fold vibration",
        "synthesis": "Subharmonic synthesis, lowered formants, chest voice"
      },
      {
        "name": "Khoomei",
        "origin": "Mongolia",
        "characteristics": "Mid-range melody with balanced drone",
        "synthesis": "F2 around 1200 Hz, equal drone/melody balance"
      },
      {
        "name": "Borbangnadyr",
        "origin": "Tuva",
        "characteristics": "Rolling, pulsating drone",
        "synthesis": "Amplitude modulation, pulsed formants"
      },
      {
        "name": "Ezengilear",
        "origin": "Altai",
        "characteristics": "Rhythmic canter pattern",
        "synthesis": "Rhythmic pulsation in canter rhythm"
      }
    ],
    "synthesis_requirements": {
      "fundamental": "Low drone pitch (typically 82-147 Hz)",
      "subharmonics": "Integer ratio subharmonics (2:1, 3:1, 4:1)",
      "formants": "Precise formant control for melody",
      "modulation": "Amplitude and frequency modulation for rhythmic styles",
      "ventricular_folds": "Simulated false vocal fold vibration for subharmonics"
    }
  },

  "synthesis_notes": {
    "subharmonic_generation": "Use oscillator sync or ring modulation to create integer-ratio subharmonics",
    "formant_synthesis": "Implement narrow bandpass filters or formant synthesis for melody",
    "drone_layer": "Separate oscillator layer for fundamental drone",
    "modulation": "LFO or envelope modulation for pulsed styles",
    "resonance": "High Q filters for whistle-like sygyt",
    "chest_voice": "Lower formant frequencies and increased bandwidth for kargyraa",
    "mixing": "Balance between drone, subharmonic, and formant layers"
  }
