  "grapheme_to_phoneme_rules": [
    {
      "pattern": "c[ei]",
      "replacement": "C_SOFT",
      "context": "before e or i (soft C)",
      "priority": 1
    },
    {
      "pattern": "c[ao]",
      "replacement": "C_HARD",
      "context": "before a or o (hard C)",
      "priority": 1
    },
    {
      "pattern": "gn",
      "replacement": "N",
      "context": "GN → /ɲ/ (palatal nasal)",
      "priority": 2
    },
    {
      "pattern": "ti[aeiou]",
      "replacement": "TI",
      "context": "TI + vowel → /tsi/",
      "priority": 2
    },
    {
      "pattern": "g[ei]",
      "replacement": "G_SOFT",
      "context": "before e or i (soft G)",
      "priority": 1
    },
    {
      "pattern": "g[ao]",
      "replacement": "G_HARD",
      "context": "before a or o (hard G)",
      "priority": 1
    },
    {
      "pattern": "ph",
      "replacement": "F",
      "context": "Greek origin",
      "priority": 1
    },
    {
      "pattern": "ch",
      "replacement": "C_HARD",
      "context": "Greek loanwords",
      "priority": 1
    },
    {
      "pattern": "th",
      "replacement": "T",
      "context": "Greek loanwords",
      "priority": 1
    },
    {
      "pattern": "ae",
      "replacement": "AE",
      "context": "diphthong",
      "priority": 3
    },
    {
      "pattern": "oe",
      "replacement": "OE",
      "context": "diphthong",
      "priority": 3
    },
    {
      "pattern": "au",
      "replacement": "AU",
      "context": "diphthong",
      "priority": 3
    }
  ],

  "stress_rules": [
    {
      "pattern": ".*[aeiouy][aeiouy][bcdfghjklmnpqrstvwxz]$",
      "stress": "penultimate",
      "priority": 1,
      "description": "Penultimate stress if penultimate syllable is heavy"
    },
    {
      "pattern": ".*[aeiouy][bcdfghjklmnpqrstvwxz][bcdfghjklmnpqrstvwxz]$",
      "stress": "antepenultimate",
      "priority": 2,
      "description": "Antepenultimate stress otherwise"
    }
  ],

